{
  "name": "openstore",
  "productName": "OpenStore",
  "version": "0.2.1",
  "description": "An open app store",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "lint": "eslint --ext .js,.jsx.ts,.tsx .",
    "install-for-arm64": "npm i --target_arch=arm64",
    "postinstall-for-arm64": "npx electron-rebuild --arch arm64",
    "make-for-arm64": "npm run install-for-arm64 && electron-forge make --arch arm64",
    "install-for-x86_64": "npm i --target_arch=x64",
    "postinstall-for-x86_64": "npx electron-rebuild --arch x64",
    "make-for-x86_64": "npm run install-for-x86_64 && electron-forge make --arch x64",
    "bump-patch": "npm version patch -m \"Bump version to %s\"",
    "bump-minor": "npm version minor -m \"Bump version to %s\"",
    "bump-major": "npm version major -m \"Bump version to %s\"",
    "distribute-macos": "scripts/distribute-macos.sh"
  },
  "keywords": [],
  "author": {
    "name": "Ian Gregory"
  },
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.61",
    "@types/better-sqlite3": "^7.4.2",
    "@types/bootstrap": "^5.1.8",
    "@types/clone-deep": "^4.0.1",
    "@types/color": "^3.0.2",
    "@types/node": "^17.0.5",
    "@types/shell-quote": "^1.7.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "css-loader": "^6.5.1",
    "electron": "16.0.5",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "fork-ts-checker-webpack-plugin": "^6.5.2",
    "node-loader": "^2.0.0",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "better-sqlite3": "^7.4.5",
    "bootstrap": "^5.1.3",
    "bootswatch": "^5.1.3",
    "clone-deep": "^4.0.1",
    "color": "^4.1.0",
    "electron-context-menu": "^3.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "lit": "^2.0.2",
    "node-fetch": "^3.2.4",
    "node-pty": "^0.10.1",
    "node-stream-zip": "^1.15.0",
    "shell-quote": "^1.7.3",
    "xterm": "^5.0.0",
    "xterm-addon-fit": "^0.6.0"
  }
}
